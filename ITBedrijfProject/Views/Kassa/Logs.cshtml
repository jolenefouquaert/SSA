@model ITBedrijfProject.Models.Errorlog

@{
    ViewBag.Title = "Logs";
}
<h2>@ViewBag.Register.RegisterName</h2>
<h3>Logs</h3>

@using (Html.BeginForm("Index", "Kassa", FormMethod.Post))
{
    <table class="table">
        <tr>
            <th>Tijdstip</th>
            <th>Bericht</th>
            <th>Stacktrace</th>
        </tr>
        @foreach (var item in @ViewBag.Log)
        {
            <tr>
                <td>@item.Timestamp</td>
                <td>@item.Message</td>
                <td>@item.Stacktrace</td>
            </tr>
        }
    </table>

    
    <input class="btn-default" type="submit" name="Back" value="Terug" />
}

